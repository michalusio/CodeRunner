<Application
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:FormClient"
             xmlns:System="clr-namespace:System;assembly=mscorlib" x:Class="FormClient.App"
             StartupUri="LoginWindow.xaml" ShutdownMode="OnLastWindowClose">
    <Application.Resources>

        <System:String x:Key="DefaultProgram" xml:space="preserve">public class Program
{
    public static void Main(PlayerData player)
    {
        var dir = (Random.Integer() % 5) - 2;
        player.Robots[0].Move((Direction) dir);
    }
}
        </System:String>

    </Application.Resources>
</Application>
